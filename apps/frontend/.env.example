# Frontend .env example

# Backend base URL.
# Local default: http://127.0.0.1:5001
# Staging example: https://backend-staging.example.com
# Production example: https://backend.example.com
NEXT_PUBLIC_API_URL=http://127.0.0.1:5001

# API keys used by server-side API routes to call the backend.
# Priority: PRODUCTION_API_KEY > STAGING_API_KEY > LOCAL_API_KEY
# Do not prefix with NEXT_PUBLIC_ and restart the dev server after changes.
PRODUCTION_API_KEY=
STAGING_API_KEY=
LOCAL_API_KEY=

# Optional: enable bundle analyzer
# ANALYZE=true