{
  "name": "@basedguide/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run pre-build-check && next dev",
    "build": "pnpm run pre-build-check && node scripts/build-monitor.js 'pnpm run build:next'",
    "build:next": "next build",
    "build:force": "pnpm run clean && pnpm run pre-build-check && node scripts/build-monitor.js 'pnpm run build:next'",
    "build:safe": "pnpm run clean && pnpm run pre-build-check && node scripts/build-monitor.js 'pnpm run build:next'",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "ANALYZE=true pnpm run build",
    "clean": "node scripts/build-clean.js",
    "pre-build-check": "node scripts/pre-build-check.js"
  },
  "dependencies": {
